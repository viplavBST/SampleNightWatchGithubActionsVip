{
  "name": "nightwatch-browserstack",
  "version": "0.1.0",
  "readme": "NightwatchJS tests on [BrowserStack](https://www.browserstack.com)",
  "description": "An example module to run nightwatch tests on browserstack",
  "scripts": {
    "test": "./node_modules/.bin/nightwatch",
    "test_local": "node scripts/test_local.js",
    "test_single": "./node_modules/.bin/nightwatch -e chrome_48",
    "test_parallel": "./node_modules/.bin/nightwatch -e default,firefox_45,chrome_48"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/browserstack/nightwatch-browserstack"
  },
  "dependencies": {
    "browserstack-local": "^0.1.0",
    "nightwatch": "^0.8.18"
  },
  "license": "MIT"
}
